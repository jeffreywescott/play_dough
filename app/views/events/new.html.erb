<h2>Create An Event</h2>


<h3>Your New Survey </h3>
<%= form_for @event do |f| %>
  <p><%= f.label :Event %></p>

  <p>Title: <%= f.text_field :title, :placeholder => "Enter your title here.."  %></p>
  <p>Description: <%= f.text_field :description, :placeholder => "Enter your description here.."  %></p>
   <p>Url: <%= f.text_field :url, :placeholder => "Enter your url here.."  %></p>

  <p><%= f.fields_for :EventTime do |event_time|  %></p>
    <%= render "event_times", :f => event_time %> 
  <% end %>
  <p><%= f.submit %></p>
<% end %>



